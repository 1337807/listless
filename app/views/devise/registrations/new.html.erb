<div class='container'>
  <div class='col-md-4'>
    <br/>
    <%= form_for(resource, as: resource_name, url: registration_path(resource_name), layout: :basic) do |f| %>
      <div class="field">
        <%= f.email_field :email, autofocus: true %>
      </div>

      <div class="field">
        <%= f.password_field :password, autocomplete: "off" %>
      </div>

      <div class="actions">
        <%= f.submit "Sign up" %>
      </div>
    <% end %>
  </div>
</div>
